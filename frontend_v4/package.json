{
  "name": "docmanager-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "python -m uvicorn backend.server:server --reload --port 8000",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:pytest": "python -m pytest -q -rA"
  },
